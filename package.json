{
  "name": "svga-web",
  "version": "2.1.7",
  "description": "SVGA Web Player",
  "homepage": "https://github.com/Naeemo/svga-web",
  "bugs": {
    "url": "https://github.com/Naeemo/svga-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Naeemo/svga-web.git"
  },
  "keywords": [
    "svga",
    "svga web player",
    "svga.lite"
  ],
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/svga-web.umd.js",
  "module": "./dist/svga-web.es.js",
  "exports": {
    ".": {
      "import": "./dist/svga-web.es.js",
      "require": "./dist/svga-web.umd.js"
    }
  },
  "types": "src/index.ts",
  "scripts": {
    "prestart": "prettier --check ./src && tsc -p ./",
    "start": "eslint ./src && vite",
    "prebuild": "prettier --check ./src && tsc -p ./",
    "build": "vite build",
    "prepublish": "yarn build",
    "proto": "node scripts/generate-proto.js"
  },
  "author": "naeemo<naeemo@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.17.9",
    "@types/offscreencanvas": "^2019.6.3",
    "@types/pako": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "np": "^7.5.0",
    "pako": "^2.0.4",
    "prettier": "^2.3.2",
    "protobufjs": "^6.11.2",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|js|json|md|html)": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
