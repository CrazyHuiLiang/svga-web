{
  "name": "svga-web",
  "version": "2.0.3",
  "description": "SVGA Web Player",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/svga-web.umd.js",
  "module": "./dist/svga-web.es.js",
  "exports": {
    ".": {
      "import": "./dist/svga-web.es.js",
      "require": "./dist/svga-web.umd.js"
    }
  },
  "types": "src/index.ts",
  "scripts": {
    "prestart": "prettier --check ./src && tsc -p ./",
    "start": "eslint ./src && vite",
    "prebuild": "prettier --check ./src && tsc -p ./",
    "build": "vite build",
    "prepublish": "yarn build",
    "proto": "node scripts/generate-proto.js"
  },
  "author": "naeemo<naeemo@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/offscreencanvas": "^2019.6.2",
    "@types/pako": "^1.0.1",
    "@types/protobufjs": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "np": "^7.4.0",
    "pako": "^2.0.3",
    "prettier": "^2.2.1",
    "protobufjs": "^6.10.2",
    "typescript": "^4.2.3",
    "vite": "^2.1.2"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|js|json|md|html)": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
